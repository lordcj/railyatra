# Environment variables template
# Copy this to .env.production and fill in your actual values

# Railway API Configuration
VITE_RAILWAY_API_BASE_URL=https://indianrailapi.com/api/v2
VITE_API_TIMEOUT=15000
VITE_ENABLE_CACHING=true
VITE_CACHE_DURATION=3600000

# RailRadar API Configuration (Primary API)
# Get your API key from: https://railradar.in/register
# Free tier: 1000 requests/month
VITE_RAILRADAR_API_KEY=your_railradar_api_key_here

# Google AdSense Configuration
# IMPORTANT: Replace with your actual AdSense Publisher ID
# Get it from: https://www.google.com/adsense
VITE_ADSENSE_CLIENT_ID=ca-pub-8792747031835209

# Production Settings
NODE_ENV=production
